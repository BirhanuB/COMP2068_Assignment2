extends layout

block content
  h1 Post Item
  form(method='POST' action='/insert')
    div
        h2 Title
        input(type='text' name='title')
    div
        h2 Description
        input(type='text' name='description')    
    div
        h2 Price
        input(type='text' name='price')
    div
        button(type='submit' value='Submit') Submit